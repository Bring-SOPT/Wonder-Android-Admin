<?xml version="1.0" encoding="utf-8"?>

<android.support.v7.widget.CardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="250dp"
        android:layout_height="350dp"
        app:cardCornerRadius="16dp"
        android:elevation="0dp"
        android:background="@color/white"
        app:cardBackgroundColor="@color/white">
    <android.support.constraint.ConstraintLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="250dp"
            android:layout_height="350dp" tools:layout_editor_absoluteY="25dp">


        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/textView2" android:id="@+id/linearLayout2"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp">
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="64dp"
                    android:id="@+id/btn_waiting_accept_dialog_5m"
                    tools:layout_editor_absoluteY="51dp"
                    tools:layout_editor_absoluteX="196dp"

                    android:background="@drawable/waiting_minute">

                <TextView
                        android:text="5분"
                        android:textColor="@color/white"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/textView3"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        android:textSize="24sp"/>
            </android.support.constraint.ConstraintLayout>
            <Space
                    android:layout_width="12dp"
                    android:layout_height="wrap_content"/>
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="64dp"
                    android:id="@+id/btn_waiting_accept_dialog_10m"
                    android:background="@drawable/waiting_minute" tools:layout_editor_absoluteY="51dp"
                    tools:layout_editor_absoluteX="84dp">

                <TextView
                        android:text="10분"
                        android:textColor="@color/white"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView5" android:textSize="24sp"/>
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/linear2"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout2" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp">
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="64dp"
                    android:id="@+id/btn_waiting_accept_dialog_15m" android:background="@drawable/waiting_minute"
                    tools:layout_editor_absoluteY="51dp" tools:layout_editor_absoluteX="196dp">

                <TextView
                        android:textColor="@color/white"
                        android:text="15분"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView6" android:textSize="24sp"/>
            </android.support.constraint.ConstraintLayout>
            <Space
                    android:layout_width="12dp"
                    android:layout_height="wrap_content"/>
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="64dp"
                    android:id="@+id/btn_waiting_accept_dialog_20m"
                    android:background="@drawable/waiting_minute" tools:layout_editor_absoluteY="51dp"
                    tools:layout_editor_absoluteX="84dp">

                <TextView
                        android:text="20분"
                        android:textColor="@color/white"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView7" android:textSize="24sp"/>
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/linear3"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/linear2" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp">
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="64dp"
                    android:id="@+id/btn_waiting_accept_dialog_25m" android:background="@drawable/waiting_minute"
                    tools:layout_editor_absoluteY="51dp" tools:layout_editor_absoluteX="196dp">

                <TextView
                        android:text="25분"
                        android:layout_width="wrap_content"
                        android:textColor="@color/white"
                        android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView11" android:textSize="24sp"/>
            </android.support.constraint.ConstraintLayout>
            <Space
                    android:layout_width="12dp"
                    android:layout_height="wrap_content"/>
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="64dp"
                    android:id="@+id/btn_waiting_accept_dialog_30m"
                    android:background="@drawable/waiting_minute" tools:layout_editor_absoluteY="51dp"
                    tools:layout_editor_absoluteX="84dp">

                <TextView
                        android:text="30분"
                        android:layout_width="wrap_content"
                        android:textColor="@color/white"
                        android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView10" android:textSize="24sp"/>
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
        <TextView
                android:text="예상 제조 완료 시간을 선택해주세요."
                android:fontFamily="@font/nanumsquareroundotfr"
                android:textSize="15dp"
                android:textColor="#707070"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:id="@+id/textView2"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"/>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/linear3">
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="48dp"
                    android:id="@+id/btn_waiting_accept_dialog_cancle"
                    android:background="#eeeeee"
                    tools:layout_editor_absoluteY="261dp" tools:layout_editor_absoluteX="84dp">

                <EditText
                        android:hint="시간입력"
                        android:background="@null"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" android:id="@+id/et_dialog_waiting_writeminute"/>
            </android.support.constraint.ConstraintLayout>
            <Space
                    android:layout_width="12dp"
                    android:layout_height="wrap_content" android:layout_weight="1"/>
            <android.support.constraint.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="48dp"
                    android:id="@+id/btn_waiting_accept_dialog_ok"
                    android:background="@drawable/waiting_minute"
                    tools:layout_editor_absoluteY="261dp" tools:layout_editor_absoluteX="196dp">

                <TextView
                        android:text="직접입력"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" android:id="@+id/tv_dialog_waiting_minuteselfselect"
                        android:textSize="20sp" android:textColor="@color/white"/>
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
    </android.support.constraint.ConstraintLayout>
</android.support.v7.widget.CardView>